{
  "name": "awssat/imageai",
  "description": "Wrapper for imageAi in PHP",
  "keywords": [
    "awssat",
    "imageAi"
  ],
  "homepage": "https://github.com/awssat/imageAi",
  "license": "MIT",
  "authors": [
    {
      "name": "Bader Almutairi",
      "email": "bderemail@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1|^8.1",
    "intervention/image": "^2.4",
    "symfony/filesystem": "^6.0",
    "symfony/process": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Awssat\\ImageAi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Awssat\\ImageAi\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
